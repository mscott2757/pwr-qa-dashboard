$("#trigger-build-<%= test.id %>").click(function () {
  <% if test.parameterized %>
      $.ajax({
        type: "POST",
        dataType: "jsonp",
        url: "<%= test.build_url %>",
        data: {"parameter": [{"name":"env","value":"<%= test.env_tag.name %>"}]}
        success: function() {
          console.log("hooray!");
        }
      });
  <% else %>
      $.ajax({
        type: "POST",
        dataType: "jsonp",
        url: "<%= test.build_url %>",
        success: function() {
          console.log("hooray!");
        }
      });
  <% end %>
});
